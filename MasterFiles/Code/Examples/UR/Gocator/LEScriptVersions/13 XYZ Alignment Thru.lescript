# 13 XYZ Alignment Thru

clear_variables()
filename = {sysSequenceFilename}

count=5
loop:
movel_rel_part({origin})
movel_rel_part(0.025,-0.108,0,0,0,0)

# Random +/- 0.25"
random(3,-0.0064,0.0064)
movel_incr_part({rnd1},{rnd2},{rnd3},0,0,0)

gocator_ID=random_0.25in_offset
